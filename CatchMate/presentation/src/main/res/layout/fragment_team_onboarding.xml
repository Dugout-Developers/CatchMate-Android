                app:cardCornerRadius="8dp"

                app:cardElevation="0dp"
                app:cardMaxElevation="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/cv_team_onboarding_doosan"
                app:layout_constraintEnd_toStartOf="@+id/cv_team_onboarding_hanwha"
                app:layout_constraintStart_toEndOf="@+id/cv_team_onboarding_doosan"
                app:layout_constraintTop_toTopOf="@+id/cv_team_onboarding_doosan">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_all_team_toggle_default_bg"
