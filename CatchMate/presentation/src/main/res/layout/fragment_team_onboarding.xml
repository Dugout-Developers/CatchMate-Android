                app:cardElevation="0dp"
                app:cardMaxElevation="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/cv_team_onboarding_kt"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/cv_team_onboarding_kt"
                app:layout_constraintTop_toTopOf="@+id/cv_team_onboarding_kt">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_all_team_toggle_default_bg"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingLeft="7dp"
                    android:paddingTop="15dp"
                    android:paddingRight="7dp"
                    android:paddingBottom="16dp">

                    <ImageView
                        android:id="@+id/iv_team_onboarding_hanwha"
                        android:layout_width="95dp"
                        android:layout_height="72dp"
                        android:src="@drawable/vec_all_bottom_sheet_handle_62dp" />

                    <TextView
