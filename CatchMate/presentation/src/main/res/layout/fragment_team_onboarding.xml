                android:background="@drawable/shape_all_team_toggle_selected_bg"
                app:cardCornerRadius="8dp"
                app:cardElevation="0dp"
                app:cardMaxElevation="0dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/cv_team_onboarding_pacifist"
                app:layout_constraintHorizontal_chainStyle="spread_inside"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cv_team_onboarding_lotte">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_all_team_toggle_default_bg"
                    android:gravity="center"
                    android:orientation="vertical"
                    android:paddingLeft="7dp"
                    android:paddingTop="15dp"
                    android:paddingRight="7dp"
                    android:paddingBottom="16dp">

                    <ImageView
                        android:id="@+id/iv_team_onboarding_kiwoom"
                        android:layout_width="95dp"
                        android:layout_height="72dp"
                        android:src="@drawable/vec_all_bottom_sheet_handle_62dp" />

                    <TextView
                        android:id="@+id/tv_team_onboarding_kiwoom"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="18dp"
                        android:text="@string/team_kiwoom_heroes"
                        android:textAppearance="@style/Typography.Body02.SemiBold"
                        android:textColor="@color/grey700" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_team_onboarding_pacifist"
                android:layout_width="109dp"
                android:layout_height="wrap_content"

                android:background="@drawable/shape_all_team_toggle_selected_bg"
                app:cardCornerRadius="8dp"
                app:cardElevation="0dp"
                app:cardMaxElevation="0dp"
                app:layout_constraintBottom_toBottomOf="@+id/cv_team_onboarding_kiwoom"
                app:layout_constraintEnd_toStartOf="@+id/cv_team_onboarding_baseball_beginner"
                app:layout_constraintStart_toEndOf="@+id/cv_team_onboarding_kiwoom"
                app:layout_constraintTop_toTopOf="@+id/cv_team_onboarding_kiwoom">

                <LinearLayout
                    android:layout_width="match_parent"
